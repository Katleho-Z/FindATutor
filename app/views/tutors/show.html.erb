<div class="main-container-tutor-show">
  <%= image_tag "https://files.slack.com/files-pri/T02NE0241-F05CZPGNLMV/504a0436-3.jpg", alt: "Tutor", width: 300, height: 300 %>
  <div class="tutor-show-info">
    <h2>  <%= "#{@tutor.first_name} #{@tutor.last_name}" %> </h2>
   <p>
      <% @tutor.skills.each do | skill | %>
        <%= skill.name %>
      <% end %>
    </p>
    <p> <%= "Location: #{@tutor.location}" %> </p>
    <p> <%= "Email Address: #{@tutor.email_address}" %> </p>
    <div class="tutor-show-reviews">
      <p> Reviews </p>
      <p> 4.5 </p>
    </div>
    <p class="black-btn"> Messages </p>
  </div>
  <div class="tutor-show-bio">
   <h3> Bio </h3>
    <p> <%= @tutor.bio %> </p>
  </div>
</div>
